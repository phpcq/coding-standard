{
    "name": "phpcq/coding-standard",
    "description": "PHP code quality project coding standard",
    "homepage": "http://phpcq.org/",
    "license": "BSD-3-Clause and MIT",
    "authors": [
        {
            "name": "Christian Schiffler",
            "email": "c.schiffler@cyberspectrum.de",
            "homepage": "http://www.cyberspectrum.de",
            "role": "Developer"
        },
        {
            "name":"Tristan Lins",
            "email":"tristan@lins.io",
            "homepage":"https://tristan.lins.io",
            "role":"Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/phpcq/coding-standard/issues",
        "source": "https://github.com/phpcq/coding-standard",
        "irc": "irc://irc.freenode.org/phpcq"
    },
    "require": {
        "php": ">=5.6",
        "squizlabs/php_codesniffer": "~2.9.2"
    },
    "require-dev": {
        "phpcq/all-tasks": "^1.3@dev"
    },
    "suggest": {
        "phpcq/phpcq": "The PHP code quality project core tasks."
    },
    "autoload-dev": {
        "psr-0": {
            "PhpCodeQuality_": "tests/"
        },
        "classmap": [
            "tests-helper/phpunit/RecursiveCallbackFilterIterator.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev",
            "dev-release/2.1.0": "2.1.x-dev"
        }
    }
}
